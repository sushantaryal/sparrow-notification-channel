{
    "name": "sushantaryal/sparrow-notification-channel",
    "description": "Sparrow SMS Notification Channel for laravel.",
    "keywords": [
        "laravel",
        "notifications",
        "sparrowsms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sushant Aryal",
            "email": "sushant.aryal90@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/notifications": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.2",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.4|^11.5"
    },
    "autoload": {
        "psr-4": {
            "Sushant\\SparrowSmsNotification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sushant\\SparrowSmsNotification\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sushant\\SparrowSmsNotification\\SparrowSmsServiceProvider"
            ]
        }
    }
}
